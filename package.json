{
  "name": "@airporting/middy-plaid",
  "version": "1.0.7",
  "main": "index.js",
  "repository": "git@github.com:airporting/middy-plaid.git",
  "author": "mathrobin <mthrobin@gmail.com>",
  "packageManager": "yarn@4.3.1",
  "type": "module",
  "engines": {
    "node": "18.18.2"
  },
  "scripts": {
    "commit": "cz",
    "lint": "eslint -c eslint.config.js",
    "postupgrade": "git add package.json yarn.lock",
    "prepare": "yarn airnpm && husky install",
    "prettier": "prettier --check ./",
    "prettier:write": "prettier --write ./",
    "publish": "yarn npm publish && git add package.json yarn.lock && git commit -m 'feat(version): bump' && git push && git status",
    "test": "yarn node --experimental-vm-modules $(yarn bin jest) --runInBand --config jest.config.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "plaid": "26.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.24.9",
    "@babel/eslint-parser": "7.25.1",
    "@babel/plugin-syntax-import-assertions": "7.24.7",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@eslint/js": "9.8.0",
    "@jest/globals": "29.7.0",
    "@ngneat/falso": "7.2.0",
    "@types/jest": "29.5.12",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "date-fns": "3.6.0",
    "eslint": "9.8.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest": "28.6.0",
    "eslint-plugin-json": "4.0.0",
    "globals": "15.8.0",
    "husky": "9.1.4",
    "jest": "29.7.0",
    "lint-staged": "15.2.8",
    "prettier": "3.3.3"
  }
}
